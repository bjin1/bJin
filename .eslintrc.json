{
    "env": {
      "node": true,
      "mocha": true,
      "es2021": true
    },
    "parser": "babel-eslint",
    "extends": ["plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "func-names": "off",
      "no-console": "off"
    }
  }
  